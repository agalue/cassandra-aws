    </service>
  </package>
  <package name="vmware6" remote="false">
    <filter>(IPADDR != '0.0.0.0') &amp; (categoryName == 'VMware6')</filter>
    <service name="VMware-VirtualMachine" interval="30000" user-defined="false" status="on">
      <parameter key="collection" value="default-VirtualMachine6"/>
      <parameter key="thresholding-enabled" value="true"/>
    </service>
    <service name="VMware-HostSystem" interval="30000" user-defined="false" status="on">
